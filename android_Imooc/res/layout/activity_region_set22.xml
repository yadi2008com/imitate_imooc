<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:paddingBottom="20sp"
    android:paddingLeft="0sp"
    android:paddingRight="0sp"
    tools:context=".RegionSet22Activity" >
    
    <include layout="@layout/back_title" />

    <RelativeLayout
        android:id="@+id/Re_Region41"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region41"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="北海道"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

    <RelativeLayout
        android:id="@+id/Re_Region42"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region42"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="本州"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

    <RelativeLayout
        android:id="@+id/Re_Region43"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region43"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="四国"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

    <RelativeLayout
        android:id="@+id/Re_Region44"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region44"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="九州"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

</LinearLayout>