<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:paddingBottom="20sp"
    android:paddingLeft="0sp"
    android:paddingRight="0sp"
    tools:context=".RegionSetActivity" >
    
    <include layout="@layout/back_title" />

    <RelativeLayout
        android:id="@+id/Re_Region1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region1"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="北京"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

    <RelativeLayout
        android:id="@+id/Re_Region2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region2"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="天津"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

    <RelativeLayout
        android:id="@+id/Re_Region3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region3"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="山西"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

    <RelativeLayout
        android:id="@+id/Re_Region4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/Tv_Region4"
            android:layout_width="wrap_content"
            android:layout_height="20sp"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="20sp"
            android:text="日本"
            android:textColor="#000" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1sp"
        android:background="#BEBEBE" >
    </View>

</LinearLayout>